{
    "name": "spatie/laravel-artisan-dd",
    "description": "Run dd from your commandline",
    "keywords": [
        "spatie",
        "laravel-artisan-dd",
        "artisan",
        "debug",
        "develop"
    ],
    "homepage": "https://github.com/spatie/laravel-artisan-dd",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/console": "5.5.x-dev",
        "illuminate/support": "5.5.x-dev"
    },
    "require-dev": {
        "orchestra/testbench": "3.5.x-dev",
        "phpunit/phpunit": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\ArtisanDd\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\ArtisanDd\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
