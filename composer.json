{
    "name": "spatie/laravel-artisan-dd",
    "description": "Run dd from your commandline",
    "keywords": [
        "spatie",
        "laravel-artisan-dd",
        "artisan",
        "debug",
        "develop"
    ],
    "homepage": "https://github.com/spatie/laravel-artisan-dd",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^7.30|^8.73|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.20|^6.23|^7.0",
        "pestphp/pest": "^1.22",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\ArtisanDd\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\ArtisanDd\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
